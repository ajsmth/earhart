{
  "name": "Earhart",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "test": "jest",
    "lint": "eslint .",
    "link:android": "adb shell am start -W -a android.intent.action.VIEW -d",
    "link:ios": "xcrun simctl openurl booted",
    "shake:android": "adb shell input keyevent 82"
  },
  "dependencies": {
    "history": "4",
    "react": "^16.11.0",
    "react-native": "^0.62.3",
    "react-native-gesture-handler": "^1.6.0",
    "react-native-rename": "^2.4.1",
    "react-native-screens": "^2.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "@testing-library/jest-native": "^3.0.2",
    "@testing-library/react-native": "^5.0.0",
    "@types/jest": "^25.1.4",
    "@types/react-native": "^0.60.17",
    "babel-jest": "^24.9.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "escape-string-regexp": "^2.0.0",
    "eslint": "^6.4.0",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.55.0",
    "react-devtools": "3",
    "react-test-renderer": "^16.11.0",
    "ts-jest": "^25.2.1"
  },
  "jest": {
    "preset": "@testing-library/react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|react-native-screens|react-native-gesture-handler)/)"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/react-native/cleanup-after-each",
      "./mock.js"
    ],
    "transform": {
      "^.+\\.(t)sx?$": "ts-jest"
    }
  }
